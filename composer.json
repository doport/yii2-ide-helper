{
    "name": "iiifx-production/yii2-autocomplete-helper",
    "type": "extension",
    "description": "Yii2 Autocomplete Helper",
    "keywords": [
        "yii2",
        "yii",
        "framework",
        "ide",
        "autocomplete",
        "helper"
    ],
    "homepage": "https://github.com/iiifx-production/yii2-autocomplete-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy IIIFX Khomenko",
            "email": "iiifx@yandex.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "yiisoft/yii2": "~2.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "iiifx\\Yii2\\Autocomplete\\": "source/"
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "v1.0.x-dev"
        }
    }
}
